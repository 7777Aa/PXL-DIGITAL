  }else{//Non-Personalized Ads
    gdpr_apps_removeCookie('adnetwork-status');
    gdpr_apps_setPersonalizedAd(1);
  }
}
function arrayContainStr(arr, str){
  var isExist = false;
  for(var i=0; i< arr.length; i++){
    if(arr[i] == str){
      isExist = true;break;
    }
  }
  return isExist;
}
